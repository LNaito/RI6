  "compilerOptions": {
    "outDir": "out/",
    "module": "commonjs",
    "target": "ES5",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["src/**/*"]
  
}
